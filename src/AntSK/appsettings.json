  "DBConnection": {
    "DbType": "PostgreSQL",
    "ConnectionStrings": "Host=10.2.14.111;Port=5432;Database=tccsk;Username=postgres;Password=471e8153"
  },
  "KernelMemory": {
    "VectorDb": "Postgres",
    "ConnectionString": "Host=10.2.14.111;Port=5432;Database=tccsk;Username=postgres;Password=471e8153",
